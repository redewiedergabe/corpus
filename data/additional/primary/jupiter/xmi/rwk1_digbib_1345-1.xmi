Type xmi:id="540" sofa="10620" begin="3393" end="3406" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="546" sofa="10620" begin="3407" end="3445" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="552" sofa="10620" begin="3446" end="3490" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="558" sofa="10620" begin="3513" end="3561" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg46.2.11##"/><type:TeiType xmi:id="564" sofa="10620" begin="3513" end="3560" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="570" sofa="10620" begin="3584" end="3723" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg46.2.12##"/><type:TeiType xmi:id="576" sofa="10620" begin="3584" end="3689" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="582" sofa="10620" begin="3690" end="3721" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Frame xmi:id="588" sofa="10620" begin="153" end="171" Pos="start" Frame="Ziepe fuhr sie an:" FrameID="1"/><rw:Frame xmi:id="595" sofa="10620" begin="383" end="392" Pos="end" Frame="sagte sie" FrameID="2"/><rw:Frame xmi:id="602" sofa="10620" begin="1044" end="1059" Pos="end" Frame="dachte Mareile." FrameID="5"/><rw:Frame xmi:id="609" sofa="10620" begin="1423" end="1437" Pos="start" Frame="und schimpfte:" FrameID="6"/><rw:Frame xmi:id="616" sofa="10620" begin="1626" end="1638" Pos="mid" Frame="bemerkte er." FrameID="7"/><rw:Frame xmi:id="623" sofa="10620" begin="2450" end="2464" Pos="end" Frame="sagte Günther." FrameID="13"/><rw:Frame xmi:id="630" sofa="10620" begin="2507" end="2532" Pos="mid" Frame="meinte Berkow und gähnte." FrameID="14"/><rw:Frame xmi:id="637" sofa="10620" begin="2783" end="2799" Pos="mid" Frame="berichtete Hans." FrameID="18"/><rw:Frame xmi:id="644" sofa="10620" begin="3545" end="3560" Pos="end" Frame="meinte Günther." FrameID="23"/><rw:Stwr xmi:id="651" sofa="10620" begin="172" end="313" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Was, heute wieder das Scheuerweib gespielt? Ich will das nicht. Ich hab' kein Scheuerweib geheiratet. Wozu is das Mädchen da – Teufel auch!«" StwrID="1"/><rw:Stwr xmi:id="664" sofa="10620" begin="365" end="382" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wie das glänzt!«" StwrID="2"/><rw:Stwr xmi:id="677" sofa="10620" begin="463" end="518" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="froh, seiner vornehmen Tochter aus den Augen zu kommen." StwrID="3"/><rw:Stwr xmi:id="690" sofa="10620" begin="728" end="799" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="erzählte dabei von der Wirtschaft, den Herrschaften, dem Dienstmädchen." StwrID="4"/><rw:Stwr xmi:id="703" sofa="10620" begin="951" end="1043" Medium="thought" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Das findet man draußen in der Welt nicht. Und doch, warum ist all das so zum Weinen traurig?" StwrID="5"/><rw:Stwr xmi:id="716" sofa="10620" begin="1438" end="1558" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Du kleines, schwarzes Aas, heißt das Setzen? Du brauchst nur mit deiner Rotznase anzustoßen, dann purzelt die Bude um.«" StwrID="6"/><rw:Stwr xmi:id="729" sofa="10620" begin="1598" end="1624" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Hier ist ungleich gemäht«" StwrID="7"/><rw:Stwr xmi:id="742" sofa="10620" begin="1639" end="1709" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Haben die Leute auch zu trinken? Ich will, daß nichts versäumt wird.«" StwrID="7"/><rw:Stwr xmi:id="755" sofa="10620" begin="1975" end="1994" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="war er unzufrieden." StwrID="8"/><rw:Stwr xmi:id="768" sofa="10620" begin="1995" end="2190" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Das müßte anders sein. Er müßte anders auf seine Leute wirken. Diese gleichgültigen Augen wollte er nicht. Teufel! Wenn man auf seine eigenen Arbeiter nicht wirkt, wo will man denn Effekt machen!" StwrID="9"/><rw:Stwr xmi:id="781" sofa="10620" begin="2018" end="2057" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border="percept" Metaph=" " Stwr="Er müßte anders auf seine Leute wirken." StwrID="10"/><rw:Stwr xmi:id="794" sofa="10620" begin="2110" end="2157" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Wenn man auf seine eigenen Arbeiter nicht wirkt" StwrID="11"/><rw:Stwr xmi:id="807" sofa="10620" begin="2285" end="2411" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Wie kam all das hierher? Der blau und weiß gestreifte Sommerflanell, das blauseidene Hemd, der rot und blau gestreifte Gürtel?" StwrID="12"/><rw:Stwr xmi:id="820" sofa="10620" begin="2435" end="2448" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Hans Berkow«" StwrID="13"/><rw:Stwr xmi:id="833" sofa="10620" begin="2488" end="2505" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Morjen, Tarniff«" StwrID="14"/><rw:Stwr xmi:id="846" sofa="10620" begin="2533" end="2546" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wie geht's?«" StwrID="14"/><rw:Stwr xmi:id="859" sofa="10620" begin="2570" end="2591" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Was machst du hier?«" StwrID="15"/><rw:Stwr xmi:id="872" sofa="10620" begin="2615" end="2641" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Siesta. Nimm doch Platz.«" StwrID="16"/><rw:Stwr xmi:id="885" sofa="10620" begin="2699" end="2771" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Was der Anblick von Hans Berkow nicht alles an Berliner Luft mitbrachte!" StwrID="17"/><rw:Stwr xmi:id="898" sofa="10620" begin="2772" end="2781" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Studien«" StwrID="18"/><rw:Stwr xmi:id="911" sofa="10620" begin="2800" end="2920" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Ich wollte euer brutales Licht studieren, dicke Bauernmädchen.Das ewige Malen von Berlinerinnen macht den Pinsel flau.«" StwrID="18"/><rw:Stwr xmi:id="924" sofa="10620" begin="2944" end="2988" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wo wohnst du? Warum bist du nicht bei uns?«" StwrID="19"/><rw:Stwr xmi:id="937" sofa="10620" begin="3012" end="3108" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Es ist nicht angenehm, der unvermeidliche Berkow zu sein. In Berlin ist er – in Kaltin wieder.«" StwrID="20"/><rw:Stwr xmi:id="950" sofa="10620" begin="3148" end="3186" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border="percept" Metaph=" " Stwr="daß es zuerst leer und starr erschien." StwrID="21"/><rw:Stwr xmi:id="963" sofa="10620" begin="3362" end="3489" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Ich wohne in einem Waldkruge. Schöne Bäume. Auch die Familie Mankow ist malerisch. Die Tochter Eva – eine gute Studie in Rot.«" StwrID="22"/><rw:Stwr xmi:id="976" sofa="10620" begin="3513" end="3543" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Dort kannst du nicht bleiben«" StwrID="23"/><rw:Stwr xmi:id="989" sofa="10620" begin="3583" end="3624" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr=" »Ja – wenn du was für mich tun willst –«" StwrID="24"/><rw:Stwr xmi:id="1002" sofa="10620" begin="3625" end="3689" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Hans blinzelte mit den roten Wimpern nachdenklich zur Sonne auf." StwrID="25"/><rw:Stwr xmi:id="1015" sofa="10620" begin="3690" end="3721" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Du hast da so 'n altes Schloß." StwrID="26"/><rw:Speaker xmi:id="1028" sofa="10620" begin="153" end="158" SpeakerID="1" Speaker="Ziepe"/><rw:Speaker xmi:id="1034" sofa="10620" begin="389" end="392" SpeakerID="2" Speaker="sie"/><rw:Speaker xmi:id="1040" sofa="10620" begin="1051" end="1058" SpeakerID="5" Speaker="Mareile"/><rw:Speaker xmi:id="1046" sofa="10620" begin="1635" end="1637" SpeakerID="7" Speaker="er"/><rw:Speaker xmi:id="1052" sofa="10620" begin="2456" end="2463" SpeakerID="13" Speaker="Günther"/><rw:Speaker xmi:id="1058" sofa="10620" begin="2514" end="2520" SpeakerID="14" Speaker="Berkow"/><rw:Speaker xmi:id="1064" sofa="10620" begin="2794" end="2798" SpeakerID="18" Speaker="Hans"/><rw:Speaker xmi:id="1070" sofa="10620" begin="3552" end="3559" SpeakerID="23" Speaker="Günther"/><rw:IntExpression xmi:id="1076" sofa="10620" begin="159" end="163" IntExpressionID="1" IntExpression="fuhr"/><rw:IntExpression xmi:id="1082" sofa="10620" begin="168" end="170" IntExpressionID="1" IntExpression="an"/><rw:IntExpression xmi:id="1088" sofa="10620" begin="383" end="388" IntExpressionID="2" IntExpression="sagte"/><rw:IntExpression xmi:id="1094" sofa="10620" begin="1044" end="1050" IntExpressionID="5" IntExpression="dachte"/><rw:IntExpression xmi:id="1100" sofa="10620" begin="1427" end="1436" IntExpressionID="6" IntExpression="schimpfte"/><rw:IntExpression xmi:id="1106" sofa="10620" begin="1626" end="1634" IntExpressionID="7" IntExpression="bemerkte"/><rw:IntExpression xmi:id="1112" sofa="10620" begin="2450" end="2455" IntExpressionID="13" IntExpression="sagte"/><rw:IntExpression xmi:id="1118" s