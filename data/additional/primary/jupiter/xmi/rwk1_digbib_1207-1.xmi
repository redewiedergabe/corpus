e xmi:id="318" sofa="9737" begin="3042" end="3182" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="324" sofa="9737" begin="3183" end="3235" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="330" sofa="9737" begin="3256" end="3325" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg74.3.334##"/><type:TeiType xmi:id="336" sofa="9737" begin="3256" end="3323" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Stwr xmi:id="342" sofa="9737" begin="208" end="272" Medium="speech" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="die wahrscheinlich viel weniger als Aga zu sagen vermocht hätten" StwrID="1"/><rw:Stwr xmi:id="355" sofa="9737" begin="274" end="346" Medium="speech" RType="indirect" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="wie sie zu dem gekommen waren, was diese ihre Verliebtheit genannt hatte" StwrID="2"/><rw:Stwr xmi:id="368" sofa="9737" begin="305" end="346" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="was diese ihre Verliebtheit genannt hatte" StwrID="3"/><rw:Stwr xmi:id="381" sofa="9737" begin="503" end="535" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Wie sollte sie ihn wohl anreden?" StwrID="4"/><rw:Stwr xmi:id="394" sofa="9737" begin="503" end="535" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Wie sollte sie ihn wohl anreden?" StwrID="5"/><rw:Stwr xmi:id="407" sofa="9737" begin="560" end="576" Medium="writing" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Lieber Freund!«" StwrID="6"/><rw:Stwr xmi:id="420" sofa="9737" begin="716" end="800" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Ach, wie anders hatte sie sich doch vorhin das Schreiben dieses Briefes vorgestellt!" StwrID="7"/><rw:Stwr xmi:id="433" sofa="9737" begin="721" end="800" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="wie anders hatte sie sich doch vorhin das Schreiben dieses Briefes vorgestellt!" StwrID="8"/><rw:Stwr xmi:id="446" sofa="9737" begin="759" end="787" Medium="writing" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="das Schreiben dieses Briefes" StwrID="9"/><rw:Stwr xmi:id="459" sofa="9737" begin="773" end="787" Medium="writing" RType="reported" Level="4" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="dieses Briefes" StwrID="10"/><rw:Stwr xmi:id="472" sofa="9737" begin="821" end="843" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und wie sie das dachte" StwrID="11"/><rw:Stwr xmi:id="485" sofa="9737" begin="845" end="899" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="formten sich ihr auch schon die Worte unter der Feder:" StwrID="12"/><rw:Stwr xmi:id="498" sofa="9737" begin="900" end="1077" Medium="writing" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Es sollte ein ganz andrer Brief werden, ich wollte Ihnen sagen – aber es ist ja nun ganz einerlei, was ich Ihnen sagen wollte, denn es bleibt nur das, was ich Ihnen sagen muß.«" StwrID="13"/><rw:Stwr xmi:id="511" sofa="9737" begin="941" end="963" Medium="writing" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="ich wollte Ihnen sagen" StwrID="14"/><rw:Stwr xmi:id="524" sofa="9737" begin="1000" end="1026" Medium="writing" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="was ich Ihnen sagen wollte" StwrID="15"/><rw:Stwr xmi:id="537" sofa="9737" begin="1052" end="1076" Medium="writing" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="was ich Ihnen sagen muß." StwrID="16"/><rw:Stwr xmi:id="550" sofa="9737" begin="1098" end="1136" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und dann schrieb sie ihm alles, alles." StwrID="17"/><rw:Stwr xmi:id="563" sofa="9737" begin="1159" end="1441" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="als durchschritte sie mit ihm noch &#13;&#10;einmal ihr ganzes vergangenes Leben, dessen Schatten sie gefangen hielten; als durchschritte sie auch das mit ihm, was doch nie sein würde, was sich vor ihr ausbreitete wie gelobtes Land, zu dem die Brücke führte, die sie nimmer betreten durfte." StwrID="18"/><rw:Stwr xmi:id="576" sofa="9737" begin="1461" end="1488" Medium="writing" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="warum es nicht sein konnte." StwrID="19"/><rw:Stwr xmi:id="589" sofa="9737" begin="1489" end="1520" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Die ganze Nacht schrieb sie ihm" StwrID="20"/><rw:Stwr xmi:id="602" sofa="9737" begin="1570" end="1587" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="bei dem Schreiben" StwrID="21"/><rw:Stwr xmi:id="615" sofa="9737" begin="1625" end="1647" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="während des Schreibens" StwrID="22"/><rw:Stwr xmi:id="628" sofa="9737" begin="1660" end="1709" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="wie sie in dieser Nacht in Wirklichkeit alt ward." StwrID="23"/><rw:Stwr xmi:id="641" sofa="9737" begin="1854" end="1973" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß er doch plötzlich dastehen möchte und sie in die Arme schlösse und sie sich an seiner Schulter ausschluchzen könne." StwrID="24"/><rw:Stwr xmi:id="654" sofa="9737" begin="2203" end="2275" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="War es denn nicht schon Glück, es ihm doch einmal alles sagen zu dürfen?" StwrID="25"/><rw:Stwr xmi:id="667" sofa="9737" begin="2234" end="2275" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="es ihm doch einmal alles sagen zu dürfen?" StwrID="26"/><rw:Stwr xmi:id="680" sofa="9737" begin="2276" end="2389" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und er würde es ja alles verstehen, würde fühlen, daß sie ihm da in diesem Briefe ihre sterbende Jugend schenkte." StwrID="27"/><rw:Stwr xmi:id="693" sofa="9737" begin="2276" end="2310" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und er würde es ja alles verstehen" StwrID="28"/><rw:Stwr xmi:id="706" sofa="9737" begin="2326" end="2389" Medium="thought" RType="indirect" Level="2" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="daß sie ihm da in diesem Briefe ihre sterbende Jugend schenkte." StwrID="29"/><rw:Stwr xmi:id="719" sofa="9737" begin="2341" end="2357" Medium="writing" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="in diesem Briefe" StwrID="30"/><rw:Stwr xmi:id="732" sofa="9737" begin="2643" end="2660" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="dieser eine Brief" StwrID="31"/><rw:Stwr xmi:id="745" sofa="9737" begin="2721" end="2757" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="was sie vorhin in Seligkeit gedacht:" StwrID="32"/><rw:Stwr xmi:id="758" sofa="9737" begin="2758" end="2833" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="der eine Zweck ihres armen Daseins war es, diesen einen Brief zu schreiben." StwrID="33"/><rw:Stwr xmi:id="771" sofa="9737" begin="2801" end="2833" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="diesen einen Brief zu schreiben." StwrID="34"/><rw:Stwr xmi:id="784" sofa="9737" begin="2801" end="2819" Medium="writing" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="diesen einen Brief" StwrID="35"/><rw:Stwr xmi:id="797" sofa="9737" begin="2888" end="2942" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="starrte sie lange sinnend auf die beschriebenen Bogen." StwrID="36"/><rw:Stwr xmi:id="810" sofa="9737" begin="2955" end="3041" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="als sei sie in dieser Nacht gestorben und als läge sie da in diesen Blättern begraben." StwrID="37"/><rw:Stwr xmi:id="823" sofa="9737" begin="3013" end="3031" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="in diesen Blättern" StwrID="38"/><rw:Stwr xmi:id="836" sofa="9737" begin="3062" end="3235" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß viele Menschen gestorben sein mußten, während sie den Brief geschrieben hatte, weil ja in jeder Nacht viele sterben. Die brauchten keiner Sonne Aufgang je mehr zu sehen." StwrID="39"/><rw:Stwr xmi:id="849" sofa="9737" begin="3104" end="3143" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="während sie den Brief geschrieben hatte" StwrID="40"/><rw:Stwr xmi:id="862" sofa="9737" begin="3116" end="3125" Medium="writing" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="den Brief" StwrID="41"/><rw:Frame xmi:id="875" sofa="9737" begin="208" end="273" Pos="start" Frame="die wahrscheinlich viel weniger als Aga zu sagen vermocht hätten," FrameID="2"/><rw:Frame xmi:id="882" sofa="9737" begin="577" end="605" Pos="end" Frame="begann sie dann schließlich," FrameID="6"/><rw:Frame xmi:id="889" sofa="9737" begin="845" end="899" Pos="start" Frame="formten sich ihr auch schon die Worte unter der Feder:" FrameID="13"/><rw:Frame xmi:id="896" sofa="9737" begin="1137" end=