e xmi:id="318" sofa="9377" begin="3042" end="3182" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="324" sofa="9377" begin="3183" end="3235" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="330" sofa="9377" begin="3256" end="3325" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg74.3.334##"/><type:TeiType xmi:id="336" sofa="9377" begin="3256" end="3323" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Stwr xmi:id="342" sofa="9377" begin="274" end="346" Medium="speech" RType="indirect" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="wie sie zu dem gekommen waren, was diese ihre Verliebtheit genannt hatte" StwrID="1"/><rw:Stwr xmi:id="355" sofa="9377" begin="305" end="346" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="was diese ihre Verliebtheit genannt hatte" StwrID="2"/><rw:Stwr xmi:id="368" sofa="9377" begin="503" end="535" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Wie sollte sie ihn wohl anreden?" StwrID="19"/><rw:Stwr xmi:id="381" sofa="9377" begin="560" end="576" Medium="writing" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Lieber Freund!«" StwrID="3"/><rw:Stwr xmi:id="394" sofa="9377" begin="716" end="800" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Ach, wie anders hatte sie sich doch vorhin das Schreiben dieses Briefes vorgestellt!" StwrID="4"/><rw:Stwr xmi:id="407" sofa="9377" begin="759" end="787" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="das Schreiben dieses Briefes" StwrID="5"/><rw:Stwr xmi:id="420" sofa="9377" begin="821" end="843" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und wie sie das dachte" StwrID="6"/><rw:Stwr xmi:id="433" sofa="9377" begin="845" end="899" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="formten sich ihr auch schon die Worte unter der Feder:" StwrID="20"/><rw:Stwr xmi:id="446" sofa="9377" begin="900" end="1077" Medium="writing" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Es sollte ein ganz andrer Brief werden, ich wollte Ihnen sagen – aber es ist ja nun ganz einerlei, was ich Ihnen sagen wollte, denn es bleibt nur das, was ich Ihnen sagen muß.«" StwrID="7"/><rw:Stwr xmi:id="459" sofa="9377" begin="941" end="963" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="ich wollte Ihnen sagen" StwrID="8"/><rw:Stwr xmi:id="472" sofa="9377" begin="1000" end="1026" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="was ich Ihnen sagen wollte" StwrID="9"/><rw:Stwr xmi:id="485" sofa="9377" begin="1052" end="1076" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="was ich Ihnen sagen muß." StwrID="10"/><rw:Stwr xmi:id="498" sofa="9377" begin="1098" end="1136" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Und dann schrieb sie ihm alles, alles." StwrID="11"/><rw:Stwr xmi:id="511" sofa="9377" begin="1461" end="1488" Medium="speech" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="warum es nicht sein konnte." StwrID="12"/><rw:Stwr xmi:id="524" sofa="9377" begin="1489" end="1520" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Die ganze Nacht schrieb sie ihm" StwrID="13"/><rw:Stwr xmi:id="537" sofa="9377" begin="1570" end="1587" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="bei dem Schreiben" StwrID="14"/><rw:Stwr xmi:id="550" sofa="9377" begin="1605" end="1623" Medium="speech" RType="reported" Level="1" NonFact="nonFact" Prag="prag" Border=" " Metaph=" " Stwr="wie müdes Entsagen" StwrID="24"/><rw:Stwr xmi:id="563" sofa="9377" begin="1625" end="1647" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="während des Schreibens" StwrID="15"/><rw:Stwr xmi:id="576" sofa="9377" begin="2233" end="2275" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr=" es ihm doch einmal alles sagen zu dürfen?" StwrID="21"/><rw:Stwr xmi:id="589" sofa="9377" begin="2276" end="2310" Medium="thought" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Und er würde es ja alles verstehen" StwrID="22"/><rw:Stwr xmi:id="602" sofa="9377" begin="2721" end="2833" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="was sie vorhin in Seligkeit gedacht: der eine Zweck ihres armen Daseins war es, diesen einen Brief zu schreiben." StwrID="16"/><rw:Stwr xmi:id="615" sofa="9377" begin="2801" end="2833" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="diesen einen Brief zu schreiben." StwrID="23"/><rw:Stwr xmi:id="628" sofa="9377" begin="3062" end="3182" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß viele Menschen gestorben sein mußten, während sie den Brief geschrieben hatte, weil ja in jeder Nacht viele sterben." StwrID="17"/><rw:Stwr xmi:id="641" sofa="9377" begin="3104" end="3143" Medium="writing" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="während sie den Brief geschrieben hatte" StwrID="18"/><rw:Frame xmi:id="654" sofa="9377" begin="208" end="273" Pos="start" Frame="die wahrscheinlich viel weniger als Aga zu sagen vermocht hätten," FrameID="1"/><rw:Frame xmi:id="661" sofa="9377" begin="577" end="604" Pos="end" Frame="begann sie dann schließlich" FrameID="3"/><rw:Frame xmi:id="668" sofa="9377" begin="1442" end="1460" Pos="start" Frame="Und sie sagte ihm," FrameID="12"/><rw:Frame xmi:id="675" sofa="9377" begin="3042" end="3061" Pos="s