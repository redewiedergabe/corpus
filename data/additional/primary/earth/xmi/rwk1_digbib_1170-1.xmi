:TeiType xmi:id="426" sofa="11047" begin="3624" end="3658" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="432" sofa="11047" begin="3660" end="3711" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="438" sofa="11047" begin="3736" end="3834" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg767.2.26##"/><type:TeiType xmi:id="444" sofa="11047" begin="3737" end="3832" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="450" sofa="11047" begin="3858" end="4068" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg767.2.27##"/><type:TeiType xmi:id="456" sofa="11047" begin="3859" end="4065" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Metadata xmi:id="462" sofa="11047" begin="0" end="4089" Name="earth" Source="digbib" OrigFile="Fontane,_Theodor_Unterm_Birnbaum.tg766_1.xml" Year="1885" Decade="1880" Title="Unterm Birnbaum" Author="Fontane, Theodor" Fictional="yes" SampleID="1" Narrative="yes" TextType="Erzähltext" Dialect="no" Perspective="third" Quotes="chevron"/><rw:Frame xmi:id="480" sofa="11047" begin="311" end="325" Pos="start" Frame="als wünsch er," FrameID="1"/><rw:Frame xmi:id="487" sofa="11047" begin="1150" end="1188" Pos="start" Frame="so daß man sich füglich fragen durfte," FrameID="4"/><rw:Frame xmi:id="494" sofa="11047" begin="2888" end="2900" Pos="start" Frame="nicht weißt," FrameID="15"/><rw:Frame xmi:id="501" sofa="11047" begin="3531" end="3612" Pos="start" Frame="erst nach einer Weile nahm er den Kranz, der über der Stuhllehne hing, und sagte:" FrameID=""/><rw:Stwr xmi:id="508" sofa="11047" begin="188" end="200" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="nachdenklich" StwrID="22"/><rw:Stwr xmi:id="521" sofa="11047" begin="326" end="414" Medium="thought" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="sie stünden noch am selben Fleck oder es wären neue statt ihrer aus dem Boden gewachsen." StwrID="1"/><rw:Stwr xmi:id="534" sofa="11047" begin="417" end="447" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Er zählte dann die Fässerreihe" StwrID="2"/><rw:Stwr xmi:id="547" sofa="11047" begin="449" end="511" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="rief, im Vorübergehen, einen kurzen Befehl in den Laden hinein" StwrID="3"/><rw:Stwr xmi:id="560" sofa="11047" begin="1189" end="1217" Medium="speech" RType="indirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="wie das alles hierher komme." StwrID="4"/><rw:Stwr xmi:id="573" sofa="11047" begin="2133" end="2191" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Du hier, Ursel!   Und Kränze!   Wer hat denn Geburtstag?«" StwrID="5"/><rw:Stwr xmi:id="586" sofa="11047" begin="2219" end="2350" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Niemand.   Es ist nicht Geburtstag.   Es ist bloß Sterbetag, Sterbetag deiner Kinder.   Aber du vergißt alles.   Bloß dich nicht.«" StwrID="6"/><rw:Stwr xmi:id="599" sofa="11047" begin="2308" end="2330" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Aber du vergißt alles." StwrID="23"/><rw:Stwr xmi:id="612" sofa="11047" begin="2333" end="2349" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Bloß dich nicht." StwrID="24"/><rw:Stwr xmi:id="625" sofa="11047" begin="2404" end="2635" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr=" »Ach, Ursel, laß doch.   Ich habe meinen Kopf voll Wunder.   Du mußt mir nicht Vorwürfe machen.   Und dann die Kinder.   Nun ja, sie sind tot, aber ich kann nicht trauern und klagen, daß sie's sind.   Umgekehrt, es ist ein Glück.«" StwrID="7"/><rw:Stwr xmi:id="638" sofa="11047" begin="2484" end="2499" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Vorwürfe machen" StwrID="8"/><rw:Stwr xmi:id="651" sofa="11047" begin="2580" end="2602" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="klagen, daß sie's sind" StwrID="9"/><rw:Stwr xmi:id="664" sofa="11047" begin="2663" end="2689" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Ich verstehe dich nicht.«" StwrID="10"/><rw:Stwr xmi:id="677" sofa="11047" begin="2664" end="2688" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Ich verstehe dich nicht." StwrID="11"/><rw:Stwr xmi:id="690" sofa="11047" begin="2717" end="2809" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Und ist nur zu gut zu verstehn.   Ich weiß nicht aus noch ein und habe Sorgen über Sorgen.«" StwrID="12"/><rw:Stwr xmi:id="703" sofa="11047" begin="2752" end="2779" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border="feel" Metaph=" " Stwr="Ich weiß nicht aus noch ein" StwrID="25"/><rw:Stwr xmi:id="716" sofa="11047" begin="2784" end="2807" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border="feel" Metaph=" " Stwr="habe Sorgen über Sorgen" StwrID="13"/><rw:Stwr xmi:id="729" sofa="11047" begin="2837" end="3478" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Worüber?   Weil du nichts Rechtes zu tun hast und nicht weißt, wie du den Tag hinbringen sollst.   Hinbringen, sag ich, denn ich will dich nicht kränken und von Zeit totschlagen sprechen.   Aber sage selbst, wenn drüben die Weinstube voll ist, dann fehlt dir nichts.   Ach, das verdammte Spiel, das ewige Knöcheln und Tempeln.   Und wenn du noch glücklich spieltest!   Ja, Hradscheck, das muß ich dir sagen, wenn du spielen willst, so spiele wenigstens glücklich.   Aber ein Wirt, der nicht glücklich spielt, muß davonbleiben, sonst spielt er sich von Haus und Hof.   Und dazu das Trinken, immer der schwere Ungar, bis in die Nacht hinein.«" StwrID="14"/><rw:Stwr xmi:id="742" sofa="11047" begin="2901" end="2934" Medium="thought" RType="indirect" Level="2" NonFact="nonFact" Prag=" " Border="state" Metaph=" " Stwr="wie du den Tag hinbringen sollst." StwrID="15"/><rw:Stwr xmi:id="755" sofa="11047" begin="2937" end="2956" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Hinbringen, sag ich" StwrID="16"/><rw:Stwr xmi:id="768" sofa="11047" begin="2995" end="3024" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="von Zeit totschlagen sprechen" StwrID="17"/><rw:Stwr xmi:id="781" sofa="11047" begin="3028" end="3104" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Aber sage selbst, wenn drüben die Weinstube voll ist, dann fehlt dir nichts." StwrID="18"/><rw:Stwr xmi:id="794" sofa="11047" begin="3223" end="3244" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="das muß ich dir sagen" StwrID="19"/><rw:Stwr xmi:id="807" sofa="11047" begin="3506" end="3525" Medium="speech" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Er antwortete nicht" StwrID="20"/><rw:Stwr xmi:id="820" sofa="11047" begin="3613" end="3709" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Hübsch.   Alles, was du machst, hat Schick.   Ach, Ursel, ich wollte, du hättest bessere Tage.«" StwrID="21"/><rw:IntExpression xmi:id="833" sofa="11047" begin="315" end="321" IntExpressionID="1" IntExpression="wünsch"/><rw:IntExpression xmi:id="839" sofa="11047" begin="1174" end="1180" IntExpressionID="4" IntExpression="fragen"/><rw:IntExpression xmi:id="845" sofa="11047" begin="3606" end="3611" IntExpressionID="" IntExpression="sagte"/><rw:Speaker xmi:id="851" sofa="11047" begin="322" end="324" SpeakerID="1" Speaker="er"/><rw:Speaker xmi: