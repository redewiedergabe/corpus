s=""/><type:TeiType xmi:id="228" sofa="8868" begin="2251" end="2687" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="234" sofa="8868" begin="2688" end="3163" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="240" sofa="8868" begin="3177" end="4379" TagName="p" Attributes=""/><type:TeiType xmi:id="246" sofa="8868" begin="3177" end="4078" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="252" sofa="8868" begin="4079" end="4377" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Stwr xmi:id="258" sofa="8868" begin="821" end="927" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„O welch ein entsetzliches Gras! Und das esset Jhr und die Leute&#13;&#10;                        in Eurem Lande?“" StwrID="1"/><rw:Stwr xmi:id="271" sofa="8868" begin="953" end="1006" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Ja, und sie lieben es mehr, als den süßesten Honig!“" StwrID="2"/><rw:Stwr xmi:id="284" sofa="8868" begin="1077" end="1279" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Seltsames Volk! Es scheint gut zu sein, und ich könnte es lieben;&#13;&#10;                        aber dennoch möchte ich dort nicht wohnen, wo die Menschen so etwas&#13;&#10;                        genießen können!“" StwrID="3"/><rw:Stwr xmi:id="297" sofa="8868" begin="1303" end="1634" Medium="speech" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Sollte ich noch eine andere Schwäche nennen, die den Baschkiren&#13;&#10;                        und andere Bewohner der Steppe eben so sehr auszeichnet, wie&#13;&#10;                        seine leidenschaftliche Vorliebe für einzelne Nährstoffe, so wäre es das&#13;&#10;                        mangelhafte Verständniß für die Begriffe von Mein und Dein" StwrID="8"/><rw:Stwr xmi:id="310" sofa="8868" begin="1547" end="1634" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="das&#13;&#10;                        mangelhafte Verständniß für die Begriffe von Mein und Dein" StwrID="9"/><rw:Stwr xmi:id="323" sofa="8868" begin="2290" end="2465" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß sie bei solchen Begriffen über Recht und Unrecht sich frei und offen und auf die&#13;&#10;                        drolligste Weise zu den verschiedenartigsten Diebstählen bekennen" StwrID="6"/><rw:Stwr xmi:id="336" sofa="8868" begin="2603" end="2687" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="wenn sie die&#13;&#10;                        dabei entwickelte Schlauheit zum Besten geben." StwrID="10"/><rw:Stwr xmi:id="349" sofa="8868" begin="2612" end="2669" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="die&#13;&#10;                        dabei entwickelte Schlauheit" StwrID="18"/><rw:Stwr xmi:id="362" sofa="8868" begin="2835" end="2864" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="ein Dieb gescholten zu werden" StwrID="7"/><rw:Stwr xmi:id="375" sofa="8868" begin="2954" end="3031" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="zu einer Kriminal=Untersuchung&#13;&#10;                        Veranlassung zu geben" StwrID="11"/><rw:Stwr xmi:id="388" sofa="8868" begin="3545" end="4380" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Jch erinnere mich hier noch an einen Handel, der zwischen einem&#13;&#10;                        hochgestellten russischen General und einem Baschkiren stattfand, und&#13;&#10;                        bei welchem Letzterer mit glühenden Worten und hinreißender Begeisterung alle die Schwierigkeiten schilderte, die er bestanden, und die&#13;&#10;                        Klugheit, die er habe entwickeln müssen, um den listigen und wachsamen Gegner zu täuschen, daher der geforderte Preis wohl als gerechtfertigt und nicht zu hoch gegriffen erscheinen möchte. Der Käufer&#13;&#10;                        resp. der General dennoch handelnd, tadelte die Beschaffenheit der Waare&#13;&#10;                        und hielt es nicht für unmöglich, daß dieselben Sachen, die er jetzt&#13;&#10;                        zu kaufen im Begriff stehe, auf seinem eigenen Revier gestohlen seien.&#13;&#10;&#13;" StwrID="13"/><rw:Stwr xmi:id="401" sofa="8868" begin="3839" end="4078" Medium="speech" RType="indirect" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="die er bestanden, und die&#13;&#10;                        Klugheit, die er habe entwickeln müssen, um den listigen und wachsamen Gegner zu täuschen, daher der geforderte Preis wohl als gerechtfertigt und nicht zu hoch gegriffen erscheinen möchte." StwrID="14"/><rw:Stwr xmi:id="414" sofa="8868" begin="3861" end="3929" Medium="thought" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="die&#13;&#10;                        Klugheit, die er habe entwickeln müssen" StwrID="17"/><rw:Stwr xmi:id="427" sofa="8868" begin="4151" end="4187" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="tadelte die Beschaffenheit der Waare" StwrID="15"/><rw:Stwr xmi:id="440" sofa="8868" begin="4247" end="4377" Medium="thought" RType="indirect" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß dieselben Sachen, die er jetzt&#13;&#10;                        zu kaufen im Begriff stehe, auf seinem eigenen Revier gestohlen seien." StwrID="16"/><rw:Frame xmi:id="453" sofa="8868" begin="928" end="939" Pos="end" Frame="seufzte er." FrameID="1"/><rw:Frame xmi:id="460" sofa="8868" begin="1007" end="1021" Pos="end" Frame="erwiderte ich." FrameID="2"/><rw:Frame xmi:id="467" sofa="8868" begin="1280" end="1289" Pos="end" Frame="sagte er." FrameID="3"/><rw:Frame xmi:id="474" sofa="8868" begin="3700" end="3838" Pos="start" Frame="und&#13;&#10;                        bei welchem Letzterer mit glühenden Worten und hinreißender Begeisterung alle die Schwierigkeiten schilderte," FrameID="14"/><rw:Frame xmi:id="481" sofa="8868" begin="4079" end="4246" Pos="start" Frame="Der Käufer&#13;&#10;                        resp. der General dennoch handelnd, tadelte die Beschaffenheit der Waare&#13;&#10;                        und hielt es nicht für unmöglich," FrameID="16"/><rw:IntExpression xmi:id="488" sofa="8868" begin="928" end="935" IntExpressionID="1" IntExpression="seufzte"/><rw:IntExpression xmi:id="494" sofa="8868" begin="1007" end="1016" IntExpressionID="2" IntExpression="erwiderte"/><rw:IntExpression xmi:id="500" sofa="8868" begin="1280" end="1285" IntExpressionID="3" IntExpression="sagte"/><rw:IntExpression xmi:id="506" sofa="8868" begin="3827" end="3837" IntExpressionID="14" IntExpression="schilderte"/><rw:IntExpression xmi:id="512" sofa="8868" begin="4217" end="4222" IntExpressionID="16" IntExpression="hielt"/><rw:IntExpression xmi:id="518" sofa="8868" begin="4232" end="4235" IntExpressionID="16" IntExpression="für"/><rw:Speaker xmi:id="524" sofa="8868" begin="936" end="938" SpeakerID="1" Speaker="er"/><rw:Speaker xmi:id="530" sofa="8868" begin="1017" end="1020" SpeakerID="2" Speaker="ich"/><rw:Speaker xmi:id="536" sofa="8868" begin="1286" end="1288" SpeakerID="3" Speaker="er"/><rw:Speaker xmi:id="542" sofa="8868" begin="3741" end="3750" SpeakerID="14" Speaker="Letzterer"/><rw:Speaker xmi:id="548" sofa="8868" begin="4083" end="4089" SpeakerID="16" Speaker="Käufer"/><rw:Metadata xmi:id="554" sofa="8868" begin="0" end="4387" Name="earth" Source="mkhz.sonntagsblatt" OrigFile="nn_sonntagsblatt18_1868_3.xml" Year="1868" Decade="1860" Title="Undefined" Author="Undefined" Fictional="no" SampleID="1" Narrative="yes" TextType="Erzähltext" Dialect="no" Perspective="first" Quotes="german"/><preprocessing:Token xmi:id="572" sofa="8868" begin="5" end="14