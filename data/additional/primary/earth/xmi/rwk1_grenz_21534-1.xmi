fa="10632" begin="4405" end="4621" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="450" sofa="10632" begin="4623" end="4672" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="456" sofa="10632" begin="4674" end="4679" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="462" sofa="10632" begin="4681" end="4787" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="468" sofa="10632" begin="4789" end="4947" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Stwr xmi:id="474" sofa="10632" begin="212" end="251" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Als Hilfslehrer bekommt er sehr wenig.&quot;" StwrID="1"/><rw:Stwr xmi:id="487" sofa="10632" begin="296" end="422" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Ja, was soll das aber?   Was wollen Sie sagen?   In welcher Hinsicht&#13;&#10;                        halten Sie ihn für verdächtig?&quot;" StwrID="2"/><rw:Stwr xmi:id="500" sofa="10632" begin="322" end="343" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Was wollen Sie sagen?" StwrID="50"/><rw:Stwr xmi:id="513" sofa="10632" begin="346" end="421" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="In welcher Hinsicht&#13;&#10;                        halten Sie ihn für verdächtig?" StwrID="3"/><rw:Stwr xmi:id="526" sofa="10632" begin="467" end="510" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Nein, nein, Sie sind da auf dem Holzwege,&quot;" StwrID="5"/><rw:Stwr xmi:id="539" sofa="10632" begin="594" end="662" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Mir drängte sich der Verdacht gleich bei seinen ersten Worten auf,&quot;" StwrID="6"/><rw:Stwr xmi:id="552" sofa="10632" begin="595" end="660" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Mir drängte sich der Verdacht gleich bei seinen ersten Worten auf" StwrID="8"/><rw:Stwr xmi:id="565" sofa="10632" begin="632" end="656" Medium="speech" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="bei seinen ersten Worten" StwrID="7"/><rw:Stwr xmi:id="578" sofa="10632" begin="722" end="746" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Aber welcher Verdacht?&quot;" StwrID="9"/><rw:Stwr xmi:id="591" sofa="10632" begin="778" end="864" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Woran denken Sie?&#13;&#10;                        Doch nicht gar, daß Boris Stepanowitsch -&quot;" StwrID="10"/><rw:Stwr xmi:id="604" sofa="10632" begin="779" end="796" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Woran denken Sie?" StwrID="11"/><rw:Stwr xmi:id="617" sofa="10632" begin="822" end="863" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Doch nicht gar, daß Boris Stepanowitsch -" StwrID="42"/><rw:Stwr xmi:id="630" sofa="10632" begin="909" end="926" Medium="speech" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Er endigte nicht." StwrID="43"/><rw:Stwr xmi:id="643" sofa="10632" begin="982" end="1151" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Ich denke es noch nicht.   Ich habe fürs erste nur unwillkürlichen Verdacht.&#13;&#10;                        Aber eine Haussuchung auf frischer Tat wäre vielleicht am Platze.&quot;" StwrID="12"/><rw:Stwr xmi:id="656" sofa="10632" begin="983" end="1007" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Ich denke es noch nicht." StwrID="13"/><rw:Stwr xmi:id="669" sofa="10632" begin="1010" end="1059" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Ich habe fürs erste nur unwillkürlichen Verdacht." StwrID="14"/><rw:Stwr xmi:id="682" sofa="10632" begin="1196" end="1213" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Unsinn, Unsinn!&quot;" StwrID="15"/><rw:Stwr xmi:id="695" sofa="10632" begin="1259" end="1611" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Er war hier, als die Nachbarn schreien hörten.   Sie kamen erst dazu, als&#13;&#10;                        die Helfershelfer bereits fort und in Sicherheit waren, und er blieb, um die&#13;&#10;                        Nachforschungen in die Ferne zu lenken.   Seine Behauptungen, die Fabel vom&#13;&#10;                        Hunde — alles das ist mir sehr, sehr verdächtig.&quot;" StwrID="16"/><rw:Stwr xmi:id="708" sofa="10632" begin="1503" end="1610" Medium="thought" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Seine Behauptungen, die Fabel vom&#13;&#10;                        Hunde — alles das ist mir sehr, sehr verdächtig." StwrID="17"/><rw:Stwr xmi:id="721" sofa="10632" begin="1503" end="1521" Medium="speech" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Seine Behauptungen" StwrID="18"/><rw:Stwr xmi:id="734" sofa="10632" begin="1656" end="1691" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Jetzt hatte Schejin voll begriffen." StwrID="19"/><rw:Stwr xmi:id="747" sofa="10632" begin="1742" end="1764" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Boris Stepanowitsch!&quot;" StwrID="20"/><rw:Stwr xmi:id="760" sofa="10632" begin="1776" end="2155" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Okolitsch!   Der ehrlichste, bravste Mensch,&#13;&#10;                        den ich kenne!   Okolitsch, dem ich Millionen anvertrauen möchte, wenn ich sie hätte!&#13;&#10;                           &#13;&#10;             &#13;&#10;              Der soll gestohlen haben! Den wagen Sie zu beschuldigen, in meiner Gegenwart?&#13;&#10;                        Wladimir Iwanowitsch, das ist schändlich! Das ist ehrlos!&quot;&#13;&#10;" StwrID="21"/><rw:Stwr xmi:id="773" sofa="10632" begin="2018" end="2047" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Den wagen Sie zu beschuldigen" StwrID="44"/><rw:Stwr xmi:id="786" sofa="10632" begin="2198" end="2204" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Sch,&quot;" StwrID="23"/><rw:Stwr xmi:id="799" sofa="10632" begin="2233" end="2318" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„dies Gespräch darf niemand hören, auch&#13;&#10;                        Ihre Tochter nicht.&quot;" StwrID="23"/><rw:Stwr xmi:id="812" sofa="10632" begin="2234" end="2247" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="dies Gespräch" StwrID="24"/><rw:Stwr xmi:id="825" sofa="10632" begin="2363" end="2383" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Ich erkläre Ihnen,&quot;" StwrID="25"/><rw:Stwr xmi:id="838" sofa="10632" begin="2422" end="2744" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„daß ich in jeder&#13;&#10;                        Hinsicht für meinen Mieter Okolitsch bürge.   Ich weise die Anschuldigung mit&#13;&#10;                        Entrüstung zurück.   Ich warne Sie, Wladimir Iwanowitsch, und verbiete Ihnen,&#13;&#10;                        in meinem Hause noch eine Silbe gegen Boris Stepanowitsch auszusprechen.&quot;" StwrID="25"/><rw:Stwr xmi:id="851" sofa="10632" begin="2423" end="2508" Medium="speech" RType="indirect" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="daß ich in jeder&#13;&#10;                        Hinsicht für meinen Mieter Okolitsch bürge." StwrID="26"/><rw:Stwr xmi:id="864" sofa="10632" begin="2511" end="2586" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Ich weise die Anschuldigung mit&#13;&#10;                        Entrüstung zurück." StwrID="28"/><rw:Stwr xmi:id="877" sofa="10632" begin="2521" end="2538" Medium="speech" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="die Anschuldigung" StwrID="27"/><rw:Stwr xmi:id="890" sofa="10632" begin="2589" end="2624" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Ich warne Sie, Wladimir Iwanowitsch" StwrID="29"/><rw:Stwr xmi:id="903" sofa="10632" begin="2671" end="2743" Medium="speech" RType="indirect" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="in meinem Hause noch eine Silbe gegen Boris Stepanowitsch auszusprechen." StwrID="30"/><rw:Stwr xmi:id="916" sofa="10632" begin="2671" end="2743" Medium="speech" RType="reported" Level="3" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="in meinem Hause noch eine Silbe gegen Boris Stepanowitsch auszusprechen." StwrID="45"/><rw:Stwr xmi:id="929" sofa="10632" begin="2881" end="2930" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Sie erlauben sich Redensarten, Andrej Fomitsch,&quot;" StwrID="32"/><rw:Stwr xmi:id="942" sofa="10632" begin="2882" end="2911" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Sie erlauben sich Redensarten" StwrID="51"/><rw:Stwr xmi:id="955" sofa="10632" begin="3023" end="3357" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„die ich überhören will, weil Sie es&#13;&#10;                        sind.   Ich mache Ihnen nur bemerklich, erstens, daß ich in Ihrem Interesse bemüht&#13;&#10;                        bin, und zweitens, daß ich trotz Ihrem unfreundlichen Entgegenkommen fortfahren&#13;&#10;                        werde, in Ihrem Interesse allen Eifer an den Tag zu legen.&quot;" StwrID="32"/><rw:Stwr xmi:id="968" sofa="10632" begin="3125" end="3356" Medium="speech" RType="indirect" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="erstens, daß ich in Ihrem Interesse bemüht&#13;&#10;                        bin, und zweitens, daß ich trotz Ihrem unfreundlichen Entgegenkommen fortfahren&#13;&#10;                        werde, in Ihrem Interesse allen Eifer an den Tag zu legen." StwrID="46"/><rw:Stwr xmi:id="981" sofa="10632" begin="3402" end="3425" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„Wladimir Iwanowitsch,&quot;" StwrID="33"/><rw:Stwr xmi:id="994" sofa="10632" begin="3454" end="4949" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="„sparen Sie Ihre Worte,&#13;&#10;                        und vor allen Dingen, lassen Sie Ihren Verdacht fahren.   Ich finde nichts, was&#13;&#10;                        denselben im geringsten rechtfertigte.   Und glauben Sie meiner Erfahrung, Sie&#13;&#10;                        können sich argen Unannehmlichkeiten aussetzen.   Boris Stepanowitsch sieht nicht&#13;&#10;                        aus wie ein Mensch, der sich ungestraft unrecht tun ließe.   Nun hören Sie.   Nehmen&#13;&#10;                        Sie einen Schutzmann mit und machen Sie sich sogleich daran, die Leute im Flecken&#13;&#10;                        aufzusuchen, welche Pferde halten, natürlich nur die gemeinen Leute, Karrenführer,&#13;&#10;                        Lohnkutscher, Gemüsegärtner und ähnliche, zuerst die, die Sie einer solchen Tat&#13;&#10;                        fähig halten.   Besichtigen Sie die Pferde und Wagen.   Auf der Chaussee ist es&#13;&#10;                        schmutzig, in den Gassen des Fleckens noch mehr.   Die Halunken werden daran&#13;&#10;                        nicht gedacht haben, und an den Rädern des betreffenden Wagens und den Beinen&#13;&#10;                        der Pferde muß sich der frische Schmutz noch erkennen lassen.   Verstehen Sie mich?&#13;&#10;                        Ja?   Gut.   Ich bin fast überzeugt, Sie legen noch heute morgen die Hand auf&#13;&#10;                        die Schuldigen.   Besehen Sie sich bei der Gelegenheit auch die Leute selbst, und&#13;&#10;                        wo Ihnen etwas verdächtig vorkommt, halten Sie förmlich Haussuchung. &#13;&#10;" StwrID="33"/><rw:Stwr xmi:id="1007" sofa="10632" begin="3455" end="3476" Medium="speech" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="sparen Sie Ihre Worte" StwrID="47"/><rw:Stwr xmi:id="1020" sofa="10632" begin="3525" end="3558" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="lassen Sie Ihren Verdacht fahren." StwrID="48"/><rw:Stwr xmi:id="1033" sofa="10632" begin="3683" end="3759" Medium="thought" RType="indirect" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Sie&#13;&#10;                        können sich argen Unannehmlichkeiten aussetzen." StwrID="49"/><rw:Stwr xmi:id="1046" sofa="10632" begin="4198" end="4262" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="die Sie einer solchen Tat&#13;&#10;                        fähig halten." StwrID="37"/><rw:Stwr xmi:id="1059" sofa="10632" begin="4405" end="4475" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Die Halunken werden daran&#13;&#10;                        nicht gedacht haben" StwrID="38"/><rw:Stwr xmi:id="1072" sofa="10632" begin="4623" end="4642" Medium="thought" RType="reported" Level="2" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Verstehen Sie mich?" StwrID="39"/><rw:Stwr xmi:id="1085" sofa="10632" begin="4705" end="4786" Medium="thought" RType="indirect" Level="2" NonFact=" " Prag=" " Border="state" Metaph=" " Stwr="Sie legen noch heute morgen die Hand auf&#13;&#10;                        die Schuldigen." StwrID="40"/><rw:Stwr xmi:id="1098" sofa="10632" begin="4878" end="4912" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="wo Ihnen etwas verdächtig vorkommt" StwrID="41"/><rw:Frame xmi:id="1111" sofa="10632" begin="511" end="576" Pos="end" Frame="sprach der Bezirksaufseher&#13;&#10;                        nachdenklich." FrameID="5"/><rw:Frame xmi:id="1118" sofa="10632" begin="688" end="704" Pos="end" Frame="beharrte Wolski." FrameID="6"/><rw:Frame xmi:id="1125" sofa="10632" begin="747" end="775" Pos="end" Frame="fragte Schejin etwas erregt." FrameID="9"/><rw:Frame xmi:id="1132" sofa="10632" begin="1214" end="1241" Pos="end" Frame="sprach der Bezirksaufseher." FrameID="15"/><rw:Frame xmi:id="1139" sofa="10632" begin="1765" end="1773" Pos="end" Frame="rief er." FrameID="20"/><rw:Frame xmi:id="1146" sofa="10632" begin="2205" end="2232" Pos="mid" Frame="mahnte der Bezirksaufseher," FrameID="23"/><rw:Frame xmi:id="1153" sofa="10632" begin="2364" end="2382" Pos="start" Frame="Ich erkläre Ihnen," FrameID="26"/><rw:Frame xmi:id="1160" sofa="10632" begin="2384" end="2421" Pos="mid" Frame="fuhr Schejin leiser und ruhiger fort," FrameID="25"/><rw:Frame xmi:id="1167" sofa="10632" begin="2630" end="2645" Pos="start" Frame="verbiete Ihnen," FrameID="30"/><rw:Frame xmi:id="1174" sofa="10632" begin="2931" end="3022" Pos="mid" Frame="sagte er und versuchte&#13;&#10;                        sich recht kühl und würdevoll auszudrücken," FrameID="32"/><rw:Frame xmi:id="1181" sofa="10632" begin="3093" end="3124" Pos="start" Frame="Ich mache Ihnen nur bemerklich," FrameID="46"/><rw:Frame xmi:id="1188" sofa="10632" begin="3426" end="3453" Pos="mid" Frame="sprach der Bezirksaufseher," FrameID="33"/><rw:Frame xmi:id="1195" sofa="10632" begin="3649" end="3682" Pos="start" Frame="Und glauben Sie meiner Erfahrung," FrameID="49"/><rw:Frame xmi:id="1202" sofa="10632" begin="46