Type xmi:id="540" sofa="10535" begin="3393" end="3406" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="546" sofa="10535" begin="3407" end="3445" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="552" sofa="10535" begin="3446" end="3490" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="558" sofa="10535" begin="3513" end="3561" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg46.2.11##"/><type:TeiType xmi:id="564" sofa="10535" begin="3513" end="3560" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="570" sofa="10535" begin="3584" end="3723" TagName="p" Attributes="rendition=#zenoPLm4n0##xml:id=tg46.2.12##"/><type:TeiType xmi:id="576" sofa="10535" begin="3584" end="3689" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="582" sofa="10535" begin="3690" end="3721" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Frame xmi:id="588" sofa="10535" begin="153" end="171" Pos="start" Frame="Ziepe fuhr sie an:" FrameID="1"/><rw:Frame xmi:id="595" sofa="10535" begin="383" end="392" Pos="end" Frame="sagte sie" FrameID="2"/><rw:Frame xmi:id="602" sofa="10535" begin="1044" end="1059" Pos="end" Frame="dachte Mareile." FrameID="4"/><rw:Frame xmi:id="609" sofa="10535" begin="1405" end="1437" Pos="start" Frame="Ziepe stand dabei und schimpfte:" FrameID="5"/><rw:Frame xmi:id="616" sofa="10535" begin="1624" end="1638" Pos="end" Frame=", bemerkte er." FrameID="6"/><rw:Frame xmi:id="623" sofa="10535" begin="2448" end="2464" Pos="end" Frame=", sagte Günther." FrameID="10"/><rw:Frame xmi:id="630" sofa="10535" begin="2505" end="2520" Pos="end" Frame=", meinte Berkow" FrameID="11"/><rw:Frame xmi:id="637" sofa="10535" begin="2781" end="2799" Pos="end" Frame=", berichtete Hans." FrameID="15"/><rw:Frame xmi:id="644" sofa="10535" begin="3543" end="3560" Pos="end" Frame=", meinte Günther." FrameID="20"/><rw:Stwr xmi:id="651" sofa="10535" begin="172" end="313" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Was, heute wieder das Scheuerweib gespielt? Ich will das nicht. Ich hab' kein Scheuerweib geheiratet. Wozu is das Mädchen da – Teufel auch!«" StwrID="1"/><rw:Stwr xmi:id="664" sofa="10535" begin="365" end="382" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wie das glänzt!«" StwrID="2"/><rw:Stwr xmi:id="677" sofa="10535" begin="728" end="799" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="erzählte dabei von der Wirtschaft, den Herrschaften, dem Dienstmädchen." StwrID="3"/><rw:Stwr xmi:id="690" sofa="10535" begin="951" end="1043" Medium="thought" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Das findet man draußen in der Welt nicht. Und doch, warum ist all das so zum Weinen traurig?" StwrID="4"/><rw:Stwr xmi:id="703" sofa="10535" begin="1438" end="1558" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Du kleines, schwarzes Aas, heißt das Setzen? Du brauchst nur mit deiner Rotznase anzustoßen, dann purzelt die Bude um.«" StwrID="5"/><rw:Stwr xmi:id="716" sofa="10535" begin="1598" end="1624" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Hier ist ungleich gemäht«" StwrID="6"/><rw:Stwr xmi:id="729" sofa="10535" begin="1639" end="1709" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Haben die Leute auch zu trinken? Ich will, daß nichts versäumt wird.«" StwrID="7"/><rw:Stwr xmi:id="742" sofa="10535" begin="1995" end="2057" Medium="thought" RType="freeIndirect" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Das müßte anders sein. Er müßte anders auf seine Leute wirken." StwrID="8"/><rw:Stwr xmi:id="755" sofa="10535" begin="2102" end="2190" Medium="thought" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Teufel! Wenn man auf seine eigenen Arbeiter nicht wirkt, wo will man denn Effekt machen!" StwrID="9"/><rw:Stwr xmi:id="768" sofa="10535" begin="2435" end="2448" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Hans Berkow«" StwrID="10"/><rw:Stwr xmi:id="781" sofa="10535" begin="2488" end="2505" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Morjen, Tarniff«" StwrID="11"/><rw:Stwr xmi:id="794" sofa="10535" begin="2533" end="2546" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wie geht's?«" StwrID="12"/><rw:Stwr xmi:id="807" sofa="10535" begin="2570" end="2591" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Was machst du hier?«" StwrID="13"/><rw:Stwr xmi:id="820" sofa="10535" begin="2615" end="2641" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Siesta. Nimm doch Platz.«" StwrID="14"/><rw:Stwr xmi:id="833" sofa="10535" begin="2772" end="2781" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Studien«" StwrID="15"/><rw:Stwr xmi:id="846" sofa="10535" begin="2800" end="2920" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Ich wollte euer brutales Licht studieren, dicke Bauernmädchen.Das ewige Malen von Berlinerinnen macht den Pinsel flau.«" StwrID="16"/><rw:Stwr xmi:id="859" sofa="10535" begin="2944" end="2988" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Wo wohnst du? Warum bist du nicht bei uns?«" StwrID="17"/><rw:Stwr xmi:id="872" sofa="10535" begin="3012" end="3108" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="»Es ist nicht angenehm, der unvermeidliche Berkow zu sein. In Berlin ist er – in Kaltin wieder.«" StwrID="18"/><rw:Stwr xmi:id="885" sofa="10535" begin="3362" end="3489" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " St