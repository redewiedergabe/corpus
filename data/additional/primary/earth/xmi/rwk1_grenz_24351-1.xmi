end="2902" TagName="pb" Attributes="corresp=http://brema.suub.uni-bremen.de/grenzboten/periodical/pageview/335939##facs=335939##"/><type:TeiType xmi:id="222" sofa="8925" begin="2922" end="2922" TagName="cb" Attributes="type=start##"/><type:TeiType xmi:id="228" sofa="8925" begin="2942" end="3747" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="234" sofa="8925" begin="2942" end="3747" TagName="p" Attributes="prev=#ID_2961##xml:id=ID_2962##"/><type:TeiType xmi:id="240" sofa="8925" begin="3767" end="5808" TagName="p" Attributes="xml:id=ID_2963##"/><type:TeiType xmi:id="246" sofa="8925" begin="3767" end="3868" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="252" sofa="8925" begin="3869" end="3928" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="258" sofa="8925" begin="3929" end="4295" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="264" sofa="8925" begin="4296" end="4491" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="270" sofa="8925" begin="4492" end="4936" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="276" sofa="8925" begin="4937" end="5186" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="282" sofa="8925" begin="5187" end="5525" TagName="s" Attributes="sample=1##"/><type:TeiType xmi:id="288" sofa="8925" begin="5526" end="5806" TagName="s" Attributes="samp_end=true##sample=1##"/><rw:Stwr xmi:id="294" sofa="8925" begin="550" end="623" Medium="speech" RType="reported" Level="1" NonFact=" " Prag="prag" Border=" " Metaph=" " Stwr="In Deutschland&#13;&#10;                        melden sich verschiedene Stimmen." StwrID="1"/><rw:Stwr xmi:id="307" sofa="8925" begin="682" end="803" Medium="thought" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="ob sie mit den&#13;&#10;                        Friedensbedingungen einverstandem sein&#13;&#10;                        werden oder nicht" StwrID="2"/><rw:Stwr xmi:id="320" sofa="8925" begin="1780" end="1976" Medium="thought" RType="reported" Level="1" NonFact="nonFact" Prag=" " Border=" " Metaph=" " Stwr="Wer in den den Deutschen diktierten&#13;&#10;                        Friedensbedingungen irgendwelches besondere Wohlwollen der Entente für das mit&#13;&#10;                        ihr verbündete Polen sehen will" StwrID="7"/><rw:Stwr xmi:id="333" sofa="8925" begin="2028" end="2088" Medium="writing" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Wir schrieben schon von&#13;&#10;                        dem Unrecht" StwrID="3"/><rw:Stwr xmi:id="346" sofa="8925" begin="2174" end="2306" Medium="thought" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="worin wir eher&#13;&#10;                        ein Wohlwollen für — den geschlagenen&#13;&#10;                        deutschen Feind sehen müssen." StwrID="8"/><rw:Stwr xmi:id="359" sofa="8925" begin="2689" end="2778" Medium="speech" RType="reported" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Diese Angelegenheit bespricht Herr Ch.&#13;&#10;                        im Krakauer „Glos Narodu&quot;" StwrID="4"/><rw:Stwr xmi:id="372" sofa="8925" begin="2830" end="5809" Medium="speech" RType="direct" Level="1" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="Könnten die Richter der Entente &#13;&#10;                  &#13;&#10;                  &#13;&#10;                  &#13;&#10;                  uns dieses für uns unschätzbare Stückchen&#13;&#10;                        Erde nicht abgeben, welches doch seit 1000&#13;&#10;                        Jahren uns gehört, welches der preußische&#13;&#10;                        Bandit uns gestohlen und dessen einheimische&#13;&#10;                        Bevölkerung er ausgerottet und national&#13;&#10;                        vergewaltigt oder nach seinen rheinischen&#13;&#10;                        Bergwerken und Fabriken hinausgetrieben&#13;&#10;                        hat! Um eine Handvoll Deutscher, von&#13;&#10;                        denen die Hälfte slavische Namen trägt, —&#13;&#10;                        die sichtbaren Spuren polnischer Herkunft —&#13;&#10;                        nicht der polnischen Macht zu übergeben, hat&#13;&#10;                        man das Interesse eines 25 Millionen zählenden Volkes geopfert . . .&#13;&#10;                  Warum hat man also das Interesse des&#13;&#10;                        Feindes höher gestellt als das unsrige? Wo&#13;&#10;                        ist die Freundschaft für Polen? Was könnte&#13;&#10;                        die Entente, indem sie den preußischen Imperialismus liquidierte, im Verhältnis zu&#13;&#10;                        uns noch weniger tun, ohne die Schlagworte&#13;&#10;                        dem Spotte auszuliefern, welche vier Jahre&#13;&#10;                        lang von den großen Männern der Entente&#13;&#10;                        ausgesprochen wurden? Wir können doch&#13;&#10;                        nicht darum beerdigt werden wie zuzeiten&#13;&#10;                        Katharinas in diesem Europa, welches von&#13;&#10;                        Wilson verwaltet wird. Wir beginnen ein&#13;&#10;                        neues Staatsleben unter folgenden Bedingungen: Die Weichsel wird im wichtigsten&#13;&#10;                        Teil ihres Laufes nicht über polnisches&#13;&#10;                        Territorium fließen, denn auf ihr rechtes&#13;&#10;                        Ufer wird sich Preußen stützen und ihre&#13;&#10;                        Mündung wird überhaupt außerhalb des&#13;&#10;                        Umfanges unserer Staatsgrenzen liegen. Das&#13;&#10;                        Recht des freien Gebrauches der Weichsel&#13;&#10;                        seitens der Deutschen aus Ostpreußen bildet&#13;&#10;                        auf dieser polnischen Arterie ein Konduminium&#13;&#10;                        mit dem Feind. An der Mündung in die&#13;&#10;                        Ostsee pflanzt sich an der wichtigsten oder&#13;&#10;                        besser einzig und allein für uns wichtigen&#13;&#10;                        Stelle der Danziger Staat auf, der faktisch&#13;&#10;                        für Polen unabhängig ist und von den&#13;&#10;                        Deutschen regiert wird. Durch einen schmalen&#13;&#10;                        polnischen Korridor, welcher nach Putzig führt,&#13;&#10;                        haben die Preußen linker- und die Preußen&#13;&#10;                        rechterseits internationales freies „Transitum&quot;&#13;&#10;                        wie im eigenen Hause.&#13;&#10;&#13;" StwrID="5"/><rw:Stwr xmi:id="385" sofa="8925" begin="4166" end="4295" Medium="speech" RType="reported" Level="2" NonFact=" " Prag=" " Border=" " Metaph=" " Stwr="welche vier Jahre&#13;&#10;                        lang von den großen Männern der Entente&#13;&#10;                        ausgesprochen wurden?" StwrID="9"/><rw:Metadata xmi:id="398" sofa="8925" begin="0" end="5820" Name="earth" Source="grenz" OrigFile="grenzboten_341909_335407.txt_88.xml" Year="1919" Decade="1910" Title="Pressestimmen" Author="Undefined" Fictional="no" SampleID="1" Narrative="no" TextType="Kommentar" Dialect="no" Perspective="first_plural" Quotes="undef"/><rw:Frame xmi:id="416" sofa="8925" begin="2783" end="2829" Pos="start" Frame="sagt u. a.&#13;&#10;                        folgendes:" FrameID="5"/><rw:IntExpression xmi:id="423" sofa="8925" begin="2783" end="2787" IntExpressionID="5" IntExpression="sagt"/><preprocessing:Token xmi:id="429" sofa="8925" begin="5" end="15" Lemma="famz_24351" RfPos="FM" Pos="ADJA"/><preprocessing:Token xmi:id="443" sofa="8925" begin="17" end="52" Lemma="grenzboten_341909_335407.txt_88.xml" RfPos="FM" Pos="NN"/><preprocessing:Token xmi:id="457" sofa="8925" begin="54" end="58" Lemma="1919" RfPos="CARD" Pos="CARD"/><preprocessing:Token xmi:id="471" sofa="8925" begin="60" end="64" Lemma="1910" RfPos="CARD" Pos="CARD"/><preprocessing:Token xmi:id