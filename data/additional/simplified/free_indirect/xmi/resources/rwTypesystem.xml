        <featureDescription>
          <name>RType</name>
          <description>- direct,
- indirect,
- free indirect,
- reported</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Level</name>
          <description>- default value: 1,
- increment for every embedded speech,
- embedded story: -1</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>NonFact</name>
          <description>- default value: unspec (unspecified),
- neg (negated), 
- hyp (hypothetical), 
- fut (future), 
- ques (question), 
- imp (wish, imperative), 
- plan (plan, intention)</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
